<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:complexType name="Vmlist">
        <xs:sequence>
            <xs:element name="list" type="VMDetail" nillable="false" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="VMDetail">
        <xs:sequence>
            <xs:element name="id"  type="xs:int" />
            <xs:element name="OS" type="xs:string" />
            <xs:element name="NICCount" type="xs:int"/>
            <xs:element name="RAM" type="xs:int" />
        </xs:sequence>
    </xs:complexType>
</xs:schema>